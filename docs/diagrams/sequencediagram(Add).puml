@startuml
'https://plantuml.com/sequence-diagram

autonumber

User -> Ui: "inventory"
Ui -> Commands: execute(INVENTORY)
Commands -> Ui: CommandResult("now in Inventory")
Ui -> User: print to user ("now in Inventory")

User -> Ui: "add egg 1"
Ui -> Commands: execute("egg 1")
Commands -> Model : addIngredient("egg", "1")
Model -> Commands: success
Commands -> Ui: CommandResult("egg added")
Ui -> User: print to user ("egg added")

User -> Ui: "bye"
Ui -> Commands: execute()
Commands -> CatalogueContentManager: saveAllCatalogues()
CatalogueContentManager -> CatalogueContentManager: saveToFile(inventory)
CatalogueContentManager -> CatalogueContentManager: saveToFile(recipeBook)
CatalogueContentManager -> Commands: save complete
